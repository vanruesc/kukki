{
	"name": "kukki",
	"version": "1.1.0",
	"description": "A simple cookie manager.",
	"homepage": "https://github.com/vanruesc/kukki",
	"main": "build/kukki.js",
	"module": "build/kukki.esm.js",
	"sideEffects": false,
	"license": "Zlib",
	"keywords": [
		"cookie",
		"manager",
		"static",
		"client",
		"browser",
		"navigator",
		"document",
		"map"
	],
	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/kukki.git"
	},
	"bugs": {
		"url": "https://github.com/vanruesc/kukki/issues"
	},
	"files": [
		"build"
	],
	"scripts": {
		"ava": "ava",
		"lint": "npm run lint:lib && npm run lint:test",
		"lint:config": "eslint *.js",
		"lint:lib": "eslint src",
		"lint:test": "eslint test",
		"build": "rollup -c",
		"build:production": "cross-env NODE_ENV=production npm run build",
		"watch": "rollup -c -w",
		"doc": "rimraf public/docs && esdoc",
		"pretest": "npm run lint && npm run build:production",
		"test": "ava",
		"prepack": "npm test && npm run doc"
	},
	"ava": {
		"failFast": true,
		"files": [
			"test/**/*.js"
		],
		"require": [
			"esm"
		]
	},
	"eslintConfig": {
		"extends": "aether"
	},
	"devDependencies": {
		"@babel/core": "7.x.x",
		"@babel/preset-env": "7.x.x",
		"@rollup/plugin-node-resolve": "7.x.x",
		"ava": "3.x.x",
		"browser-env": "3.x.x",
		"cross-env": "7.x.x",
		"esdoc": "1.x.x",
		"esdoc-standard-plugin": "1.x.x",
		"eslint": "6.x.x",
		"eslint-config-aether": "1.x.x",
		"esm": "3.x.x",
		"rimraf": "3.x.x",
		"rollup": "1.x.x",
		"rollup-plugin-babel": "4.x.x",
		"rollup-plugin-terser": "5.x.x"
	}
}
