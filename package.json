{
	"name": "kukki",
	"version": "1.0.3",
	"description": "A simple cookie manager.",
	"homepage": "https://github.com/vanruesc/kukki",
	"main": "build/kukki.js",
	"module": "src/index.js",
	"sideEffects": false,
	"license": "Zlib",

	"keywords": [
		"cookie",
		"manager",
		"static",
		"client",
		"browser",
		"navigator",
		"document",
		"map"
	],

	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},

	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/kukki.git"
	},

	"bugs": {
		"url": "https://github.com/vanruesc/kukki/issues"
	},

	"files": [
		"src",
		"build"
	],

	"scripts": {
		"ava": "ava",
		"lint": "npm run lint:lib && npm run lint:test",
		"lint:config": "eslint *.js",
		"lint:lib": "eslint src",
		"lint:test": "eslint test",
		"build": "rollup -c",
		"build:production": "cross-env NODE_ENV=production npm run build",
		"doc": "esdoc",
		"pretest": "npm run lint && npm run build:production",
		"test": "npm run ava",
		"prepack": "npm test"
	},

	"ava": {
		"failFast": true,
		"files": ["test/**/*.js"]
	},

	"eslintConfig": {
		"extends": "delta"
	},

	"devDependencies": {
		"@babel/core": "7.x.x",
		"@babel/preset-env": "7.x.x",
		"ava": "1.x.x",
		"browser-env": "3.x.x",
		"cross-env": "5.x.x",
		"esdoc": "1.x.x",
		"esdoc-standard-plugin": "1.x.x",
		"eslint": "5.x.x",
		"eslint-config-delta": "0.x.x",
		"rollup": "1.x.x",
		"rollup-plugin-babel": "4.x.x",
		"rollup-plugin-babel-minify": "8.x.x",
		"rollup-plugin-node-resolve": "4.x.x"
	}
}
