{
	"name": "kukki",
	"version": "2.0.0",
	"description": "A simple cookie manager.",
	"homepage": "https://github.com/vanruesc/kukki",
	"main": "dist/kukki.js",
	"module": "dist/kukki.js",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": "./dist/kukki.js"
	},
	"type": "module",
	"sideEffects": false,
	"license": "Zlib",
	"keywords": [
		"cookie",
		"manager",
		"static",
		"client",
		"browser",
		"navigator",
		"document",
		"map"
	],
	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/kukki.git"
	},
	"bugs": {
		"url": "https://github.com/vanruesc/kukki/issues"
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">= 0.14.0"
	},
	"scripts": {
		"ava": "ava",
		"build": "run-s build:*",
		"build:js": "node esbuild",
		"build:dts": "tsc -p tsconfig.d.json",
		"clean": "del-cli dist docs",
		"deploy": "run-s lint doc",
		"doc": "typedoc",
		"lint": "tsc --noemit && eslint --ext .ts --fix src",
		"prepublishOnly": "run-s clean test doc",
		"test": "run-s lint build ava"
	},
	"ava": {
		"failFast": true,
		"files": [
			"test/**/*.js"
		]
	},
	"eslintConfig": {
		"root": true,
		"extends": "aether/typescript",
		"parserOptions": {
			"project": [
				"tsconfig.json"
			]
		}
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "4.x.x",
		"@typescript-eslint/parser": "4.x.x",
		"ava": "3.x.x",
		"browser-env": "3.x.x",
		"del-cli": "3.x.x",
		"esbuild": "0.x.x",
		"eslint": "7.x.x",
		"eslint-config-aether": "1.x.x",
		"npm-run-all": "4.x.x",
		"tslib": "2.x.x",
		"typedoc": "0.x.x",
		"typescript": "4.x.x"
	}
}
